class Chopper
  def chop(num, list)
=begin
    if num == 3 && list == []
      -1
    end

    @ret = 0
    list.each do |number|
      if number == num
        return ret
      end
        ret = ret + 1
    end
=end
    for i in list
      if(list[i] == num)
        return i
      end
    end

  end

end
